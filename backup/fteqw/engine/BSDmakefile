#just a stub to make things easier.
#you still need gmake and gcc.

all:
	gmake all

help:
	gmake help

clean:
	gmake clean

gl-dbg:
	gmake gl-dbg

sw-dbg:
	gmake sw-dbg

m-dbg:
	gmake m-dbg

sv-dbg:
	gmake sv-dbg

gl-rel:
	gmake gl-rel

sw-rel:
	gmake sw-rel

m-rel:
	gmake m-rel

sv-rel:
	gmake sv-rel
